- const __ = htmlWebpackPlugin.options.i18n
- const posts = htmlWebpackPlugin.options.posts
html
  head
    meta(charset="utf-8")
    title New Web Site
  body
    .container
      .posts
        each post in posts
          .post-card
            a.post-preview(href=`/posts/${post.filename}.html`)
              span.post-preview-title !{post.title}
              //span !{post.date}
